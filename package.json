{
  "name": "statustracker",
  "version": "0.0.1",
  "description": "A simple order status tracker",
  "author": "Daniel Backus",
  "license": "MIT",
  "scripts": {
    "build": "npx babel views/public/src --out-dir views/public/js",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "npm run build && ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "npm run build && nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && tsc && npm run build",
    "start:prod": "node dist/main.js",
    "start:hmr": "node dist/server",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage --coverageReporters=text-lcov | coveralls",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "webpack": "webpack --config webpack.config.js"
  },
  "dependencies": {
    "@nestjs/common": "^5.1.0",
    "@nestjs/core": "^5.1.0",
    "@nestjs/passport": "^5.1.0",
    "@nestjs/typeorm": "^5.2.2",
    "@sendgrid/mail": "^6.3.1",
    "@types/exceljs": "^0.5.2",
    "axios": "^0.18.0",
    "bcrypt": "^3.0.2",
    "class-transformer": "^0.1.10",
    "class-validator": "^0.9.1",
    "dotenv": "^6.1.0",
    "ejs": "^2.6.1",
    "event-stream": "^3.3.4",
    "exceljs": "^1.6.2",
    "express-rate-limit": "^3.2.1",
    "frameguard": "^3.0.0",
    "lodash": "^4.17.11",
    "passport": "^0.4.0",
    "passport-http-bearer": "^1.0.1",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.2.2",
    "sendgrid": "^5.2.3",
    "sqlite3": "^4.0.3",
    "streamifier": "^0.1.1",
    "typeorm": "^0.2.8",
    "typescript": "^3.0.1",
    "uuid": "^3.3.2",
    "vue": "^2.5.17",
    "vuex": "^3.0.1",
    "vuex-persist": "^2.0.0",
    "xlsx": "^0.14.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.5",
    "@babel/preset-env": "^7.1.5",
    "@nestjs/testing": "^5.1.0",
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.1",
    "@types/node": "^10.7.1",
    "@types/supertest": "^2.0.5",
    "@types/uuid": "^3.4.4",
    "babel-plugin-transform-custom-element-classes": "^0.1.0",
    "babel-plugin-transform-es2015-classes": "^6.24.1",
    "coveralls": "^3.0.2",
    "jest": "^23.5.0",
    "nodemon": "^1.18.3",
    "prettier": "^1.14.2",
    "rimraf": "^2.6.2",
    "supertest": "^3.1.0",
    "ts-jest": "^23.1.3",
    "ts-loader": "^4.4.2",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.5.0",
    "tslint": "5.11.0",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0",
    "webpack-node-externals": "^1.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
